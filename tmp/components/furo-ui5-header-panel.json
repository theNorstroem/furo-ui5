          "text": "string"
        },
        "default": "''",
        "privacy": "public",
        "description": "Header Text",
        "attribute": "header-text"
      },
      {
        "kind": "field",
        "name": "secondaryText",
        "type": {
          "text": "string"
        },
        "default": "''",
        "privacy": "public",
        "description": "sub title",
        "attribute": "secondary-text"
      },
      {
        "kind": "field",
        "name": "collapsed",
        "type": {
          "text": "boolean"
        },
        "default": "false",
        "privacy": "public",
        "attribute": "collapsed",
        "reflects": true
      }
    ],
    "attributes": [
      {
        "name": "header-text",
        "description": "Header Text",
        "fieldName": "headerText"
      },
      {
        "name": "secondary-text",
